<script>

	$.typeahead({
		input: '.form-control',
    minLength:1,
    maxItem: 8,
    order:"asc",
		hint: true,
		display: ["company","address"],
    emptyTemplate: 'No result for "{{query}}"',
    dropdownFilter: [{
    key: 'categories',
    template: '<strong>{{categories}}</strong> 城市',
    all: '任何城市'
}],
    template: '<span>' +
        '<span class="name">{{company}}</span>' +
        '<br><span class="division" style="color:#c3c3c3;">(深圳, {{address}}, {{categories}})</span>' +
        '<span class="team-logo" style="color:#999900;">' +
            '<%for(var x=0;x<5;x++){%><i class="fa fa-star" style="float:right;" aria-hidden="true"></i><%}%>' +
        '</span>' +
    '</span>',
        correlativeTemplate:true,
		source: {
			url: "/autocomplete" // Ajax request to get JSON from the action url
		},
		callback: {
      onNavigateAfter: function (node, lis, a, item, query, event) {
         if (~[38,40].indexOf(event.keyCode)) {
             var resultList = node.closest("form").find("ul.typeahead__list"),
                 activeLi = lis.filter("li.active"),
                 offsetTop = activeLi[0] && activeLi[0].offsetTop - (resultList.height() / 2) || 0;

             resultList.scrollTop(offsetTop);
         }

     },

     onResult: function (node, query, result, resultCount) {
         if (query === "") return;

         var text = "";
         if (result.length > 0 && result.length < resultCount) {
             text = "Showing <strong>" + result.length + "</strong> of <strong>" + resultCount + '</strong> elements matching "' + query + '"';
         } else if (result.length > 0) {
             text = 'Showing <strong>' + result.length + '</strong> elements matching "' + query + '"';
         } else {
             text = 'No results matching "' + query + '"';
         }
         $('#result-container').html(text);
       },
onMouseEnter: function (node, a, item, event) {

      $(a).append('<span class="flag-chart flag-' + item.display.replace(' ', '-').toLowerCase() + '"></span>')


},
onMouseLeave: function (node, a, item, event) {

  $(a).find('.flag-chart').remove();

},

			// Redirect to url after clicking or pressing enter
			onClickAfter: function (node, a, item, event) {
				window.location.href = "/id/"+item._id; // Set window location to site url
			}
		}
	});

</script>
<script>
$( function() {
  var select = $( "#price" );
  var slider = $( "<br><br><div id='slider'></div>" ).insertAfter( select ).slider({
    min: 1,
    max: 6,
    range: "min",
    value: select[ 0 ].selectedIndex + 1,
    slide: function( event, ui ) {
      select[ 0 ].selectedIndex = ui.value - 1;
    }
  });
  $( "#price" ).on( "change", function() {
    slider.slider( "value", this.selectedIndex + 1 );
  });
} );
</script>
<script>
  // Initialize Firebase
  $(document).ready(function(){
     $('select').niceSelect();
     $('#price').niceSelect('destroy');
  var price = getUrlVars()["price"];

document.getElementById("resultt").innerHTML =decodeURIComponent(price);
  function getUrlVars()
  {
      var vars = [], hash;
      var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
      for(var i = 0; i < hashes.length; i++)
      {
          hash = hashes[i].split('=');
          vars.push(hash[0]);
          vars[hash[0]] = hash[1];
      }
      return vars;
    }
  }
  )

</script>
