<script>
  // Initialize Firebase
  $(document).ready(function(){
     $('select').niceSelect();
     $('#price').niceSelect('destroy');
  var price = getUrlVars()["price"];

document.getElementById("resultt").innerHTML =decodeURIComponent(price);
  function getUrlVars()
  {
      var vars = [], hash;
      var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
      for(var i = 0; i < hashes.length; i++)
      {
          hash = hashes[i].split('=');
          vars.push(hash[0]);
          vars[hash[0]] = hash[1];
      }
      return vars;
    }
  }
  )

</script>
<script>
$( function() {
  var select = $( "#price" );
  var slider = $( "<br><br><div id='slider'></div>" ).insertAfter( select ).slider({
    min: 1,
    max: 6,
    range: "min",
    value: select[ 0 ].selectedIndex + 1,
    slide: function( event, ui ) {
      select[ 0 ].selectedIndex = ui.value - 1;
    }
  });
  $( "#price" ).on( "change", function() {
    slider.slider( "value", this.selectedIndex + 1 );
  });
} );
</script>
